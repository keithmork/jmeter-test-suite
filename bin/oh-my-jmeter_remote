#!/bin/bash

# GUI模式只应该用来调试，正式测试请用命令行

source "$(dirname "$0")/../conf/omj_config.sh"

readonly testplan="$1"
[[ -n "$2" ]] && readonly make_report="true" || readonly make_report="false"

run_test "${testplan}" "${make_report}" "$@"

# 参数说明
# http://jmeter.apache.org/usermanual/get-started.html#options
